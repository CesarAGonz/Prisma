type Order {
    id: ID! @id
    createdAt: DateTime!
    user: User! @relation(link: INLINE)
    room: Room @relation(link: INLINE)
    active: Boolean! @default(value: true)
    product: [Product] @relation(link: INLINE)
    schedule: Schedule! @relation(link: INLINE)
    sku: String!
    amount: Int!
    rewards: Int!
}

enum OrderState{
    NUEVA
    PENDIENTE
    CANCELADA
    PAGADA
    CONFIRMADA
}